{
  "name": "sensifull",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios14": "react-native run-ios --simulator iphone 14 pro",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios:dev": "react-native run-ios --scheme 'development'",
    "ios:prod": "react-native run-ios --scheme 'production'",
    "ios:staging": "react-native run-ios --scheme 'staging'",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean-project": "react-native-clean-project",
    "report": "cd android && ./gradlew signingReport && cd ..",
    "bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "postversion": "cd android && ./gradlew assembleRelease",
    "gitPush": "git push -u origin DevOnePro",
    "build-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "postinstall": "react-native setup-ios-permissions && npx pod-install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-firebase/app": "^18.5.0",
    "@react-native-firebase/auth": "^18.5.0",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-native-picker/picker": "^2.6.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/material-top-tabs": "^6.6.13",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@tanstack/react-query": "^5.28.8",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-blurhash": "^2.0.2",
    "react-native-calendars": "^1.1304.1",
    "react-native-collapsible": "^1.6.1",
    "react-native-device-info": "^10.11.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-image-picker": "^7.1.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-pager-view": "^6.2.3",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-tab-view": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "apisauce": "^3.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-hook-form": "^7.43.9",
    "react-native-responsive-screen": "^1.4.2",
    "react-redux": "^8.0.5",
    "react-test-renderer": "18.2.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "typescript": "5.0.4",
    "yup": "^1.1.1"
  },
  "engines": {
    "node": ">=18"
  }
}
